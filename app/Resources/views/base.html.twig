<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        <link rel="icon" type="image/x-icon" href="{{ asset('bundles/EPHEC/images/logo-HD.png') }}" />

        <link rel="stylesheet" href="{{ asset('bundles/EPHEC/css/bootstrap.min.css') }}"/>
        <!--<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">-->
        <link rel="stylesheet" href="{{ asset('bundles/EPHEC/css/material-fullpalette.min.css') }}"/>
        <!--<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-material-design/0.3.0/css/material-fullpalette.min.css">-->
        <link rel="stylesheet" href="{{ asset('bundles/EPHEC/css/ripples.min.css') }}"/>
        <!--<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-material-design/0.3.0/css/ripples.min.css">-->
        {#<link rel="stylesheet" href="{{ asset('bundles/EPHEC/css/roboto.min.css') }}"/>#}
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-material-design/0.3.0/css/roboto.min.css">
        <!-- Dropdown.js -->
        <link rel="stylesheet" href="{{ asset('bundles/EPHEC/css/jquery.dropdown.css') }}"/>
        <!--<link href="//cdn.rawgit.com/FezVrasta/dropdown.js/master/jquery.dropdown.css" rel="stylesheet">-->
        <link rel="stylesheet" href="{{ asset('bundles/EPHEC/css/styles3.css') }}" type="text/css" />

        <!-- jQuery -->
        <script src="{{ asset('bundles/EPHEC/js/jquery-1.10.2.min.js') }}"></script>
        <!--<script src="//code.jquery.com/jquery-1.10.2.min.js"></script>-->
        <!--<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap-theme.min.css">-->
    {% endblock %}
    {% block scriptgmap %}{% endblock %}
</head>
<body>
{% block navbar %}
    {% set path = path(app.request.attributes.get('_route'),app.request.attributes.get('_route_params')) %}
    <div class="navbar navbar-material-light-blue-500">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-responsive-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
                <a class="navbar-brand" href="{{ path('homepage') }}"><img src="{{ asset('bundles/EPHEC/images/logo-HD.png') }}" style="width:35px;height:35px;"></a>
                <a class="navbar-brand" href="{{ path('homepage') }}">Notice Me</a>
        </div>
        <div class="navbar-collapse collapse navbar-responsive-collapse">
            <ul class="nav navbar-nav">
                {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                    <li {% if path == path('ephec_note_homepage') %} class="active" {% endif %}><a href="{{ path('ephec_note_homepage') }}">My Memo</a></li>
                    {#<li><a href="{{ path('ephec_note_homepage') }}" class="ne">Pr√©sentation</a></li>#}
                    <li {% if path == path('ephec_note_archivedMemo') %} class="active" {% endif %}><a href="{{ path('ephec_note_archivedMemo') }}" class="ne">Archived Memo</a></li>
                {% endif %}
                <!--<li class="dropdown">
                    <a href="http://fezvrasta.github.io/bootstrap-material-design/bootstrap-elements.html" data-target="#" class="dropdown-toggle ne" data-toggle="dropdown">Dropdown <b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li><a href="javascript:void(0)">Action</a></li>
                        <li><a href="javascript:void(0)">Another action</a></li>
                        <li><a href="javascript:void(0)">Something else here</a></li>
                        <li class="divider"></li>
                        <li class="dropdown-header">Dropdown header</li>
                        <li><a href="javascript:void(0)">Separated link</a></li>
                        <li><a href="javascript:void(0)">One more separated link</a></li>
                    </ul>
                </li>-->
            </ul>
            <!--<form class="navbar-form navbar-left">
                <input type="text" class="form-control col-lg-8" placeholder="Recherche">
            </form>-->
            <ul class="nav navbar-nav navbar-right">
                {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                    <li class="dropdown">
                        <a href="http://fezvrasta.github.io/bootstrap-material-design/bootstrap-elements.html" data-target="#" class="dropdown-toggle ne" data-toggle="dropdown">{{ app.user.username }}<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li><a href="{{ path('fos_user_profile_show') }}">Profile</a></li>
                            <li class="divider"></li>
                            <li><a href="{{ path('fos_user_security_logout') }}">Log out</a></li>
                        </ul>
                    </li>
                {% else %}
                    {#<li {% if path == path('fos_user_registration_register') %} class="active" {% endif %}><a href="{{path('fos_user_registration_register')}}" class="ne">Sign up</a></li>#}
                    <li {% if path == path('fos_user_security_login') %} class="active" {% endif %}><a href="{{path('fos_user_security_login')}}" class="ne">Login</a></li>
                {% endif %}
            </ul>
        </div>
    </div>
{% endblock %}
{% block content %}

{% endblock %}
{% block javascripts %}
    <script src="{{ asset('bundles/EPHEC/js/bootstrap.min.js') }}"></script>
    {#<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>#}
    <script src="{{ asset('bundles/EPHEC/js/material.min.js') }}"></script>
    {#<script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-material-design/0.3.0/js/material.min.js"></script>#}
    <script src="{{ asset('bundles/EPHEC/js/ripples.min.js') }}"></script>
    {#<script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-material-design/0.3.0/js/ripples.min.js"></script>#}
    <script src="{{ asset('bundles/EPHEC/js/jquery.dropdown.js') }}"></script>
    {#<script src="https://cdn.rawgit.com/FezVrasta/dropdown.js/master/jquery.dropdown.js"></script>#}
{% endblock %}
</body>
</html>